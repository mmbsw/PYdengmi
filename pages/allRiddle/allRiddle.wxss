
.table {
  border: 0rpx solid darkgray;
}
.main{
  height: calc(100vh - 96rpx);
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 96rpx; 
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}

.th {
  font-size: 30rpx;
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 96rpx;
  align-items: center;
}